        <value>/*global tf_config, tf_workspace, tf_run, tf_terraform_record, tf_vcs_record, tf_http, tf_variable, tf_util*/&#13;
&#13;
gs.info("Workflow: Terraform Provision Workspace with Variables Started...");&#13;
&#13;
// ** Set to the Variable Set for this Catalog Item **&#13;
// ** Any variables in the set must start with `tf_`&#13;
var VAR_SET_NAME = "manage_my_vm_size_variable_set";&#13;
var tf_variables = current.variables[VAR_SET_NAME];&#13;
&#13;
/* Instantiate Terraform Classes */&#13;
var conf               = new tf_config();&#13;
var util               = new tf_util();&#13;
var workspace          = new tf_workspace();&#13;
var terraform_record   = new tf_terraform_record();&#13;
var vcs_record         = new tf_vcs_record();&#13;
var terraform_variable = new tf_variable();&#13;
&#13;
/* Variables from the Request */&#13;
var requestItemNumber = current.number;&#13;
var requestID         = current.request.getDisplayValue();       // Catalog Request ID&#13;
var workspaceVCS      = 'Manage Size of my VM'; // Variable from Catalog Request&#13;
var description       = current.variables.description;&#13;
&#13;
/* Data for the request content */&#13;
var workspaceName  = workspace.createName(requestItemNumber);&#13;
var workspaceURL   = workspace.createURL(conf, workspaceName);&#13;
var requestItemURL = terraform_record.createRecordURL(current.sys_id);&#13;
var vcsRecord      = vcs_record.get(workspaceVCS);&#13;
&#13;
/* Create request content */&#13;
var content        = workspace.createRequestContent(vcsRecord, workspaceName, requestItemNumber, requestItemURL, description, false, true);&#13;
var requestContent = JSON.stringify(content);&#13;
&#13;
/* HTTP REST request to Terraform API */&#13;
var http               = new tf_http("TF Workspaces", "Create Workspace", requestContent);&#13;
var workspace_response = http.execute();&#13;
var workspace_id       = workspace_response.data.id;&#13;
&#13;
/* Create SN Record from response */&#13;
var gr = terraform_record.create(workspace_response, current.request.sys_id, current.sys_id, workspaceURL);&#13;
&#13;
var comment = util.formatCommentBlockQuote("Terraform: Workspace Created&lt;/br&gt;provisioning resources...");&#13;
terraform_record.postRITMComment(gr.requested_item, comment);&#13;
&#13;
// Create any TF Variables/ENV Variables from the Variable Set.&#13;
terraform_variable.createVariablesFromSet(tf_variables, workspace_id);&#13;
&#13;
// Create a run for workspace&#13;
// Workspace was created with "auto-apply"&#13;
util.sleep(5000);&#13;
&#13;
var run               = new tf_run();&#13;
var runContent        = run.createRunRequestContent(workspace_id, requestID);&#13;
var runRequestContent = JSON.stringify(runContent);&#13;
&#13;
/* HTTP REST request to Terraform API */&#13;
var runHTTP  = new tf_http("TF Runs", "Create Run", runRequestContent);&#13;
runHTTP.execute();&#13;
&#13;
gs.info("Workflow: Terraform Provision Workspace with Variables Finished.");&#13;
</value>
